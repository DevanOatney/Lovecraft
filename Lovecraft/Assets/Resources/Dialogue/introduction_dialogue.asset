  m_EditorHideFlags: 0
  m_Script: {fileID: 11500000, guid: 3b1e7284cf7543d41b80dceb2f90ce8f, type: 3}
  m_Name: introduction_dialogue
  m_EditorClassIdentifier: 
  dialogueName: intro_story_dialogue
  lines:
  - speakerName: Emma
    dialogueText: Henry... I'm tired.
    portraitName: E_Neutral_ECMC
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 8300000, guid: bb1f3971227a97649bb59fd76725545e, type: 3}
    portrait: {fileID: 21300000, guid: f15a6791eb00ce341b9f8615d276b286, type: 3}
    CinematicCameraName: CineCameraPos_1
  - speakerName: Henry
    dialogueText: "I know, my love but you must keep fighting. Things will be better
      soon. You\u2019ll see."
    portraitName: H_Soft_EOMC
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: 4a12ab943ca7f0f479a7dca9e0cc285d, type: 3}
    CinematicCameraName: 
  - speakerName: Emma
    dialogueText: Help me outside. Just for a moment. I miss the garden. Do you remember
      how we would sit under the oak in the evenings? The jasmine smelled so sweet.
    portraitName: Emma_Neutral
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: 739fdbf58c9672945b33535c2b87ba7c, type: 3}
    CinematicCameraName: 
  - speakerName: Henry
    dialogueText: "Yes, I remember. But you mustn\u2019t go outside. You need to
      stay here and rest."
    portraitName: H_Soft_EOMO
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: 55666a6a9e5d25348a43073c6d771186, type: 3}
    CinematicCameraName: 
  - speakerName: 
    dialogueText: '*the clatter of armor and hooves approaching*'
    portraitName: PLACEHOLDER
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: c8a03d02e827d914cb4d071268997b30, type: 3}
    CinematicCameraName: 
  - speakerName: Henry
    dialogueText: ...
    portraitName: H_Neutral_EOMC
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: f5c0d99cd70440746b247bffdbfa49a9, type: 3}
    CinematicCameraName: CineCameraPos_3
  - speakerName: Henry
    dialogueText: Stay inside.
    portraitName: H_Neutral_EOMO
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: 0b3956ebe50615b45972502d526e2553, type: 3}
    CinematicCameraName: CineCameraPos_1
  - speakerName: High Inquisitor
    dialogueText: It is the hour of your reckoning. You slaughter our kin, use them
      as fodder for your infernal machine. Today your fiendish machinations come
      to an end.
    portraitName: Inquisitor Portrait
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: 8b7752e19a43dc649917d0224b457c21, type: 3}
    CinematicCameraName: CineCameraPos_2
  - speakerName: Henry
    dialogueText: I did what was necessary.
    portraitName: H_Angry_ECMC
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: b9bab0b4b4c220348ae0e093f7862a76, type: 3}
    CinematicCameraName: CineCameraPos_1
  - speakerName: High Inquisitor
    dialogueText: You and that witch are a plague upon this land. By grand ducal
      decree, on this day, you will be bathed in holy fire. May God have mercy on
      your soul.
    portraitName: Inquisitor Portrait
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: 8b7752e19a43dc649917d0224b457c21, type: 3}
    CinematicCameraName: CineCameraPos_2
  - speakerName: High Inquisitor
    dialogueText: Tear it down. And kill them both.
    portraitName: Inquisitor Portrait
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: 8b7752e19a43dc649917d0224b457c21, type: 3}
    CinematicCameraName: CineCameraPos_3
  - speakerName: 
    dialogueText: The soldiers surround the Beating Heart and ready their weapons.
    portraitName: PLACEHOLDER
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: c8a03d02e827d914cb4d071268997b30, type: 3}
    CinematicCameraName: 
  - speakerName: 
    dialogueText: A sword slashes down on the machine.
    portraitName: PLACEHOLDER
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: c8a03d02e827d914cb4d071268997b30, type: 3}
    CinematicCameraName: 
  - speakerName: Henry
    dialogueText: Stop! Get away from it!
    portraitName: H_Angry_EOMO
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: 3ca56baacc1d8684fa901a297bd54a70, type: 3}
    CinematicCameraName: CineCameraPos_1
  - speakerName: 
    dialogueText: The men swing their weapons wildly. A cable severs and spews blood.
    portraitName: PLACEHOLDER
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: c8a03d02e827d914cb4d071268997b30, type: 3}
    CinematicCameraName: 
  - speakerName: Emma
    dialogueText: Aaaahh!
    portraitName: E_Neutral_ECMO
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: af59190543e839f4dab0842c6c168a69, type: 3}
    CinematicCameraName: 
  - speakerName: Henry
    dialogueText: "Stop it! You\u2019re killing her!"
    portraitName: H_Angry_EOMO
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: 3ca56baacc1d8684fa901a297bd54a70, type: 3}
    CinematicCameraName: 
  - speakerName: Emma
    dialogueText: Henry!
    portraitName: E_Neutral_EOMO
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: 5fed1ad6395af11409fcdb558f1cf885, type: 3}
    CinematicCameraName: 
  - speakerName: Henry
    dialogueText: I said STOP!
    portraitName: H_Transform
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: 041929feb94a65c429c527a5435f98d5, type: 3}
    CinematicCameraName: 
  - speakerName: 
    dialogueText: Consumed by rage, Henry transforms into a beast and heaves himself
      violently into the group of men, claws rending flesh.
    portraitName: monsterhenry
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: a033074e92c2be24eabb9db4e25d6af7, type: 3}
    CinematicCameraName: 
  - speakerName: Henry
    dialogueText: Emma! Are you alright?
    portraitName: H_Neutral_ECMO
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: 022124188154cdc42a3af4d33ff98252, type: 3}
    CinematicCameraName: 
  - speakerName: Emma
    dialogueText: "Henry, what\u2019s going on?"
    portraitName: E_Neutral_EOMO
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: 5fed1ad6395af11409fcdb558f1cf885, type: 3}
    CinematicCameraName: 
  - speakerName: Henry
    dialogueText: "Just stay inside! Everything will be okay. I\u2019ll make sure
      of it."
    portraitName: H_Angry_ECMO
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: 3fc60ec13e7747b46be9135ec2bd385d, type: 3}
    CinematicCameraName: 
  - speakerName: Henry
    dialogueText: More will come. I must prepare.
    portraitName: H_Angry_EOMC
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 21300000, guid: 33f522208bb001d40b8236c8438c8a74, type: 3}
    CinematicCameraName: 
  OnCompletion:
    m_PersistentCalls:
      m_Calls: []
  ReturnCameraName: Main Camera
