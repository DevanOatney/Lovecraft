  m_Script: {fileID: 11500000, guid: 3b1e7284cf7543d41b80dceb2f90ce8f, type: 3}
  m_Name: introduction_dialogue
  m_EditorClassIdentifier: 
  dialogueName: intro_story_dialogue
  lines:
  - speakerName: Emma
    dialogueText: Henry... I'm tired.
    portraitName: Emma_Neutral
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 8300000, guid: 1d67bad16f2aba9479c6c40aa60f05b7, type: 3}
    portrait: {fileID: 0}
    CinematicCameraName: CineCameraPos_1
  - speakerName: Henry
    dialogueText: "I know, my love but you must keep fighting. Things will be better
      soon. You\u2019ll see."
    portraitName: Henry_Soft
    sfxType: 0
    isRandomBark: 0
    storyAudioClip: {fileID: 8300000, guid: 0f68d83215699ed4bba3a65959e30819, type: 3}
    portrait: {fileID: 21300000, guid: 148b8195b1182354095a5034d750f1d8, type: 3}
    CinematicCameraName: 
  - speakerName: Emma
    dialogueText: Help me outside. Just for a moment. I miss the garden. Do you remember
      how we would sit under the oak in the evenings? The jasmine smelled so sweet.
    portraitName: Emma_Neutral
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 8300000, guid: b2495b9c7301c8040aca6361d603506e, type: 3}
    portrait: {fileID: 0}
    CinematicCameraName: 
  - speakerName: Henry
    dialogueText: "Yes, I remember. But you mustn\u2019t go outside. You need to
      stay here and rest."
    portraitName: Emma_Neutral
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 8300000, guid: 3eaf1ea2f883b594cb8d196f0147b3fb, type: 3}
    portrait: {fileID: 0}
    CinematicCameraName: 
  - speakerName: Henry
    dialogueText: ...
    portraitName: Henry_Neutral
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 0}
    portrait: {fileID: 0}
    CinematicCameraName: CineCameraPos_3
  - speakerName: Henry
    dialogueText: Stay inside.
    portraitName: Henry_Neutral
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 8300000, guid: 7312dadc282d6e04fa8bff91a9a1a279, type: 3}
    portrait: {fileID: 0}
    CinematicCameraName: CineCameraPos_1
  - speakerName: High Inquisitor
    dialogueText: It is the hour of your reckoning. You slaughter our kin, use them
      as fodder for your infernal machine. Today your fiendish machinations come
      to an end.
    portraitName: Inquisitor Portrait
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 8300000, guid: 4a2cafc4b4e5b2d42a408be5e29e3edc, type: 3}
    portrait: {fileID: 0}
    CinematicCameraName: CineCameraPos_2
  - speakerName: Henry
    dialogueText: I did what I had to do.
    portraitName: Henry_Angry
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 8300000, guid: 3aa17ebc39609094e8aabcd8c5e20e8c, type: 3}
    portrait: {fileID: 0}
    CinematicCameraName: CineCameraPos_1
  - speakerName: High Inquisitor
    dialogueText: You and that witch are a plague upon this land. By grand ducal
      decree, on this day, you will be bathed in holy fire. May God have mercy on
      your soul.
    portraitName: Inquisitor Portrait
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 8300000, guid: 2bd216241cf30f44c95b3b91128022b5, type: 3}
    portrait: {fileID: 0}
    CinematicCameraName: CineCameraPos_2
  - speakerName: High Inquisitor
    dialogueText: Tear it down. And kill them both.
    portraitName: Inquisitor Portrait
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 8300000, guid: bef515840a701524ab122def23e33008, type: 3}
    portrait: {fileID: 0}
    CinematicCameraName: CineCameraPos_3
  - speakerName: Henry
    dialogueText: Stop! Get away from it!
    portraitName: Henry_Neutral
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 8300000, guid: 0cb4a6bc284838d4594c528f98bd2e68, type: 3}
    portrait: {fileID: 0}
    CinematicCameraName: CineCameraPos_1
  - speakerName: Emma
    dialogueText: Aaaahh!
    portraitName: Emma_Neutral
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 8300000, guid: 117ceac79d293704c8a0fa57a24b5f18, type: 3}
    portrait: {fileID: 0}
    CinematicCameraName: 
  - speakerName: Henry
    dialogueText: "Stop it! You\u2019re killing her!"
    portraitName: Henry_Angry
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 8300000, guid: f7af19d382b5e084592159d9f5b17ad5, type: 3}
    portrait: {fileID: 0}
    CinematicCameraName: 
  - speakerName: Emma
    dialogueText: Henry!
    portraitName: Emma_Neutral
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 8300000, guid: 3aefa81920614014c9807ce1000dc0b8, type: 3}
    portrait: {fileID: 0}
    CinematicCameraName: 
  - speakerName: Henry
    dialogueText: I said STOP!
    portraitName: Henry_Angry
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 8300000, guid: d9566b0f279cf8d48813416c78e5463a, type: 3}
    portrait: {fileID: 0}
    CinematicCameraName: 
  - speakerName: Henry
    dialogueText: Emma! Are you alright?
    portraitName: Henry_Angry
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 8300000, guid: 5e9e287b7cd1e924cbd5127839fe1089, type: 3}
    portrait: {fileID: 0}
    CinematicCameraName: 
  - speakerName: Emma
    dialogueText: "Henry, what\u2019s going on?"
    portraitName: Emma_Neutral
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 8300000, guid: dba3c4cfd9ad8004fb1b60bfd4c5995b, type: 3}
    portrait: {fileID: 0}
    CinematicCameraName: 
  - speakerName: Henry
    dialogueText: Just stay inside! Everything will be alright.
    portraitName: Henry_Angry
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 8300000, guid: 7312dadc282d6e04fa8bff91a9a1a279, type: 3}
    portrait: {fileID: 0}
    CinematicCameraName: 
  - speakerName: Henry
    dialogueText: "I\u2019ll make sure of it."
    portraitName: Henry_Neutral
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 8300000, guid: 6e4444f64d2565b4498737e341b45038, type: 3}
    portrait: {fileID: 0}
    CinematicCameraName: 
  - speakerName: Henry
    dialogueText: More will come. I must prepare.
    portraitName: Henry_Angry
    sfxType: 5
    isRandomBark: 0
    storyAudioClip: {fileID: 8300000, guid: 2353a2d11a1f56b4da756d06b76701d5, type: 3}
    portrait: {fileID: 0}
    CinematicCameraName: 
